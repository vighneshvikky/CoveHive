<h1>Edit product</h1>
<form action="/user-address-edit/<%=address.id %>" method="POST">
    <div>
        <label for="name">Name:</label>
        <input type="text" id="name" name="fullName"  value="<%= address.fullName %>"required>
      </div>

    <div>
      <label for="street">Building/Address line:</label>
      <input type="text" id="street" name="street" value="<%=address.street%>">
    </div>
  
    <div>
      <label for="city">City:</label>
      <input type="text" id="city" name="city" value="<%=address.city%>" required>
    </div>
  
    <div>
      <label for="pincode">pincode:</label>
      <input type="text" id="zip" name="pincode"  value="<%=address.pincode%>"required>
    </div>
  
    <div>
      <label for="state">State:</label>
      <input type="text" id="state" name="state" value="<%=address.state%>" required>
    </div>
  
    <div>
      <label for="country">Country:</label>
      <input type="text" id="country" name="country"  value="<%= address.country %>" required>
    </div>
  
  
    <button type="submit">Add Address</button>
  </form>
  